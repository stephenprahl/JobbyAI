{
  "version": 2,
  "buildCommand": "bun run build:client",
  "outputDirectory": "dist/client",
  "installCommand": "bun install",
  "devCommand": "bun run dev:client",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}